[flake8]
max-line-length = 88
extend-ignore = E203,W503
per-file-ignores =
    */migrations/*.py: E501
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    env,
    .env,
    node_modules,
    build,
    dist,
    .eggs,
    *.egg,
    .mypy_cache,
    .pytest_cache,
    .tox,
    .idea,
    .vscode,
    .elasticbeanstalk
